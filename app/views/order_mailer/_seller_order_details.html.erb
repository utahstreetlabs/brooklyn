<%= mailer_subheader do %>
  Order Details
<% end %>

<%= mailer_graf do %>
  Order Date: <%= date(order.created_at) %><br />
  Listing Title: <%= order.listing.title %><br />
  Order Total (You Earn): <%= number_to_currency order.listing.proceeds %><br />
  <%= mailer_order_details_tracking_number(order, tracking_number: tracking_number) if defined?(tracking_number) %>
<% end %>
